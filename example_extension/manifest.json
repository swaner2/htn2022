{
  "manifest_version": 2,
  "name": "Take Care",
  "description": "This extension will remind you to take care of yourself",
  "version": "1.0",
  "permissions" : [
    "notifications",
    "storage",
    "alarms"
],

  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "48": "48.png",
    "128": "128.png"
  }
},
   
  "background": {
  "service_worker": "background.js"
  }
}
